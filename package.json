{
  "name": "biogas-waste-marketplace",
  "version": "1.0.0",
  "description": "B2B and B2C marketplace for fruits and vegetables waste for biogas production",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm install && npm run install-client && npm run install-server"
  },
  "keywords": [
    "biogas",
    "waste",
    "marketplace",
    "b2b",
    "b2c",
    "mern"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.5.0",
    "googleapis": "^164.1.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "stripe": "^13.6.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
